-- Create an empty temp table with the correct schema
CREATE OR REPLACE TEMP TABLE _work AS
SELECT
  CAST(NULL AS STRING) AS Column_Name,
  CAST(NULL AS STRING) AS Column_Value,
  CAST(NULL AS STRING) AS Data_Type,
  CAST(NULL AS STRING) AS Sys_Of_Rec_Cd,
  CAST(NULL AS STRING) AS Src_Sys_Cd,
  CAST(NULL AS TIMESTAMP) AS Crt_Rec_Sys_Tms,
  CAST(NULL AS STRING) AS Row_Actn_Cd,
  CAST(NULL AS STRING) AS Row_Reprcs_Flg,
  CAST(NULL AS STRING) AS Row_Rej_Flg,
  CAST(NULL AS STRING) AS Prcs_Id,
  CAST(NULL AS STRING) AS Audit_Row_Id,
  CAST(NULL AS DATE) AS Bus_Dt
WHERE FALSE;
